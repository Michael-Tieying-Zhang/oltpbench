<?xml version="1.0"?>
<dialects>
    <dialect type="POSTGRES">
        <procedure name="ReadRecord">
            <statement name="readStmt">
                SELECT * FROM "HTABLE" WHERE H_KEY=?
            </statement>
        </procedure>
        <procedure name="ScanRecord">
            <statement name="scanStmt">
                SELECT * FROM "HTABLE" WHERE H_KEY &gt; ? AND H_KEY &lt; ?
            </statement>
        </procedure>
        <procedure name="UpdateRecord">
            <statement name="updateAllStmt">
                UPDATE "HTABLE" SET FIELD1=?,FIELD2=?,FIELD3=?,FIELD4=?,FIELD5=?,FIELD6=?,FIELD7=?,FIELD8=?,FIELD9=?,FIELD10=? WHERE H_KEY=?
            </statement>
        </procedure>
        <procedure name="InsertRecord">
            <statement name="insertStmt">
                INSERT INTO "HTABLE" VALUES (?,?,?,?,?,?,?,?,?,?,?)
            </statement>
        </procedure>
        <procedure name="DeleteRecord">
            <statement name="deleteStmt">
                DELETE FROM "HTABLE" where H_KEY=?
            </statement>
        </procedure>
        <procedure name="ReadModifyWriteRecord">
            <statement name="selectStmt">
                Select * from "HTABLE" where H_KEY=? FOR UPDATE
            </statement>
            <statement name="updateAllStmt">
                UPDATE "HTABLE" SET FIELD1=?,FIELD2=?,FIELD3=?,FIELD4=?,FIELD5=?,FIELD6=?,FIELD7=?,FIELD8=?,FIELD9=?,FIELD10=? WHERE H_KEY=?
            </statement>
        </procedure>
    </dialect>
</dialects>
